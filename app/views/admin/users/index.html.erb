<h1>所有使用者</h1>
<table class="table table-hover">
	<tbody>
		<% @users.each do |user|%>
			<tr>
				<th><%= link_to user.username || user.email, admin_user_path(user)%></th>
			</tr>
		<% end %>
	</tbody>
</table>

<div>
<%= link_to "Back to windows", admin_windows_path, class:"btn btn-info"%>
</div>